#define _CRT_SECURE_NO_WARNINGS 1
#include <stdio.h>
int Add(int x, int y)//Add叫函数名
{
	int z = x + y;
	return z;

}//大括号内叫函数体
int main()
{
	int num1 = 10;
	int num2 = 20;
	int sum = 0;
	int a = 100;
	int b = 200;
	//sum = num1 + num2;
	sum = Add(a, b);
	sum = Add(num1,num2);
	//sum = a + b;
	
	printf("sum=%d\n", sum);

	return 0;
}
